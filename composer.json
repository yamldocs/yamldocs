{
  "name": "yamldocs/yamldocs",
  "description": "YAML Reference Builder / Markdown Document Generator",
  "license": "MIT",
  "homepage": "https://yamldocs.dev",
  "keywords": ["documentation","yaml", "cli"],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Yamldocs\\": "src/"
    }
  },
  "require": {
    "minicli/minicli": "^4.0",
    "minicli/command-help": "^1.0",
    "symfony/yaml": "^6.1",
    "minicli/stencil": "^0.1.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "scripts": {
    "test" : ["pest"]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "bin": [
    "bin/yamldocs"
  ]
}
